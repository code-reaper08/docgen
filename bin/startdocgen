#!/usr/bin/env node
const chalk = require('chalk');
const figlet = require('figlet');
 
figlet.text('DocGen', {
    font: 'Doh',
    horizontalLayout: 'default',
    verticalLayout: 'default',
    width: 150,
    whitespaceBreak: true
}, function(err, data) {
    if (err) {
        console.log(chalk.red('Something went wrong...'));
        console.dir(err);
        return;
    }
    console.log(chalk.yellow.dim(data));
});

